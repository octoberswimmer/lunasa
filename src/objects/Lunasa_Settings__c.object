<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Next_Day_Threshold__c</fullName>
        <defaultValue>&quot;00:00:00&quot;</defaultValue>
        <description>When an event’s end time spans into another day, the minimum time it must be in
            order for it to render as if it were on that day.</description>
        <externalId>false</externalId>
        <inlineHelpText>When an event’s end time spans into another day, the minimum time it must be
            in order for it to render as if it were on that day.</inlineHelpText>
        <label>Next Day Threshold</label>
        <length>8</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Event_Color__c</fullName>
        <defaultValue>&quot;#3a87ad&quot;</defaultValue>
        <description>Hexadecimal color for the events displayed in the calendar</description>
        <externalId>false</externalId>
        <inlineHelpText>A hexadecimal color is specified with: #rrggbb Where rr (red), gg (green)
            and bb (blue) are hexadecimal integers between 00 and ff, specifying the intensity of
            the color.</inlineHelpText>
        <label>Event Color</label>
        <length>7</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Earliest_Time__c</fullName>
        <defaultValue>&quot;00:00&quot;</defaultValue>
        <description>Earliest time available for appointments</description>
        <externalId>false</externalId>
        <label>Earliest Time</label>
        <length>5</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Hide_Weekends__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Whether to include Saturday and Sunday on the calendar</description>
        <externalId>false</externalId>
        <label>Hide Weekends</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Single_Column_Mode__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Enable compact layout</description>
        <externalId>false</externalId>
        <label>Single Column Mode</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Latest_Time__c</fullName>
        <defaultValue>&quot;24:00&quot;</defaultValue>
        <description>Lastest time available for appointments</description>
        <externalId>false</externalId>
        <label>Latest Time</label>
        <length>5</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Lunasa Settings</label>
    <visibility>Public</visibility>
</CustomObject>
